{
  "name": "ddrv/container",
  "type": "library",
  "description": "Simple PSR-11 Container ",
  "keywords": ["psr-11", "di", "dependency-injection", "dependency-container", "container"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Dudarev",
      "email": "ivan@ddrv.ru",
      "homepage": "https://ddrv.ru"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/container": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "provide": {
    "psr/container-implementation": "2.0"
  },
  "autoload": {
    "psr-4": {
      "Ddrv\\Container\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Ddrv\\Container\\": "tests/"
    }
  },
  "funding": [
    {
      "type": "other",
      "url": "https://www.paypal.me/ddrv"
    }
  ]
}
